  # Comment the next line if you're not using Swift and don't want to use dynamic frameworks
  use_frameworks!

  # Pods for evchargers
  pod 'GoogleMaps'
  pod 'AFNetworking', '~> 3.0'

  target 'evchargersTests' do
    inherit! :search_paths
    # Pods for testing
  end

  target 'evchargersUITests' do
    inherit! :search_paths
    # Pods for testing
  end

end
